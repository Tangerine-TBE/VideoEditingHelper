<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/reverseContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.activity.ReverseActivity">


        <com.twx.module_videoediting.ui.widget.TitleBar
            android:id="@+id/reverseTitleBar"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            app:barTitle="倒放"
            app:barHaveTvAction="true"
            app:barTvActionTitle="导出"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <com.twx.module_videoediting.ui.widget.video.reverse.TWVideoEditReverseContainer
            android:id="@+id/mTWVideoReverseContainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/reverseTitleBar" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>