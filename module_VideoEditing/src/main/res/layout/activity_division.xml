<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>

    </data>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/divisionContainer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

        <com.twx.module_videoediting.ui.widget.TitleBar
            android:id="@+id/divisionTitleBar"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            app:barTitle="分割"
            app:barHaveTvAction="true"
            app:barTvActionTitle="导出"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <com.twx.module_videoediting.ui.widget.video.division.VideoDivisionContainer
            android:id="@+id/mVideoDivisionContainer"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/divisionTitleBar" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>