<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.twx.videoclip">

    <application
        android:allowBackup="true"
        android:name="com.twx.module_videoediting.base.VideoApplication"
        android:icon="@mipmap/app_logo"
        android:label="${app_name_channel}"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:requestLegacyExternalStorage="true"
        tools:replace="android:allowBackup,android:theme,android:label"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name="com.twx.module_videoediting.ui.activity.BeginActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/AppStartTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>